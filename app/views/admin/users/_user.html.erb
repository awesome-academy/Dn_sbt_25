<tr>
  <td><%= user.name %></td>
  <td><%= user.email %></td>
  <td><%= user.phone %></td>
  <% unless user.admin? %>
    <td>
      <span class = "detail"><%= link_to t(".btn_detail"), admin_user_path(user) %></span>
      <span class = "delete"><%= link_to t(".btn_delete"), admin_user_path(user), method: :delete, data: {confirm: t(".confirm")}%></span>
    </td>
  <% end %>
</tr>
